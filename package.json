{
  "name": "vue-project-template",
  "version": "0.1.0",
  "private": true,
  "description": "A Vue.js project (boilerplate) template.",
  "author": "Jon LaBelle",
  "bugs": {
    "url": "https://github.com/jonlabelle/vue-project-template/issues"
  },
  "homepage": "https://github.com/jonlabelle/vue-project-template",
  "keywords": [
    "vue",
    "project-template",
    "template"
  ],
  "license": "MIT",
  "repository": "jonlabelle/vue-project-template",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "build-dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "lint-fix": "vue-cli-service lint --fix",
    "test": "vue-cli-service test:unit",
    "format-check-debug": "prettier . --check --loglevel debug",
    "format-check": "prettier . --check",
    "format": "prettier --write .",
    "docker-build": "docker build --no-cache --file .docker/Dockerfile --tag vue-project-template:dev .",
    "docker": "docker run --rm --name vue-project-template --publish 8080:80 vue-project-template:dev"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "core-js": "^3.19.2",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@vue/cli-plugin-babel": "~5.0.0-rc.2",
    "@vue/cli-plugin-eslint": "~5.0.0-rc.2",
    "@vue/cli-plugin-pwa": "~5.0.0-rc.2",
    "@vue/cli-plugin-router": "~5.0.0-rc.2",
    "@vue/cli-plugin-unit-jest": "~5.0.0-rc.2",
    "@vue/cli-plugin-vuex": "~5.0.0-rc.2",
    "@vue/cli-service": "~5.0.0-rc.2",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "babel-jest": "^27.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "jest": "^27.4.0",
    "prettier": "^2.5.0",
    "sass": "^1.43.5",
    "sass-loader": "^12.3.0"
  }
}

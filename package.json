{
  "name": "vue-project-template",
  "version": "0.1.0",
  "private": true,
  "description": "A Vue.js project (boilerplate) template.",
  "author": "Jon LaBelle",
  "bugs": {
    "url": "https://github.com/jonlabelle/vue-project-template/issues"
  },
  "homepage": "https://github.com/jonlabelle/vue-project-template",
  "keywords": [
    "vue",
    "project-template",
    "template"
  ],
  "license": "MIT",
  "repository": "jonlabelle/vue-project-template",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "build-dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "docker-build": "docker build --tag jonlabelle/vue-project-template .",
    "docker-run": "docker run -it -p 8080:80 --rm --name vue-project-template jonlabelle/vue-project-template"
  },
  "dependencies": {
    "@shoelace-style/shoelace": "^2.0.0-beta.39",
    "@shoelace-style/vue-sl-model": "^1.0.0",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.0-beta3",
    "core-js": "^3.11.0",
    "vue": "^2.6.12",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@vue/babel-preset-app": "^4.5.12",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^6.2.2",
    "is-svg": ">=4.2.2",
    "prettier": "^2.2.1",
    "sass": "^1.32.11",
    "sass-loader": "^10.1.1",
    "ssri": ">=8.0.1",
    "vue-template-compiler": "^2.6.12"
  }
}

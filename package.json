{
  "name": "vue-project-template",
  "version": "0.1.0",
  "private": true,
  "description": "A Vue.js project (boilerplate) template.",
  "author": "Jon LaBelle",
  "bugs": {
    "url": "https://github.com/jonlabelle/vue-project-template/issues"
  },
  "homepage": "https://github.com/jonlabelle/vue-project-template",
  "keywords": [
    "vue",
    "project-template",
    "template"
  ],
  "license": "MIT",
  "repository": "jonlabelle/vue-project-template",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "build-dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit",
    "format-check-debug": "prettier . --check --loglevel debug",
    "format-check": "prettier . --check",
    "format": "prettier . --write",
    "docker-build": "docker build --file .docker/Dockerfile --tag vue-project-template:latest .",
    "docker-run": "docker run --rm --interactive --name vue-project-template --publish 8080:80 vue-project-template:latest",
    "docker-run-detach": "docker run --detach --rm --name vue-project-template --publish 8080:80 vue-project-template:latest"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "bootstrap": "^5.1.1",
    "core-js": "^3.18.0",
    "vue": "^3.2.6",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@vue/cli-plugin-babel": "~5.0.0-beta.5",
    "@vue/cli-plugin-eslint": "~5.0.0-beta.5",
    "@vue/cli-plugin-router": "~5.0.0-beta.5",
    "@vue/cli-plugin-unit-jest": "~5.0.0-beta.5",
    "@vue/cli-plugin-vuex": "~5.0.0-beta.5",
    "@vue/cli-service": "~5.0.0-beta.5",
    "@vue/compiler-sfc": "^3.2.19",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.18.0",
    "jest": "^27.0.5",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0"
  }
}

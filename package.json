{
  "name": "vue-project-template",
  "version": "0.1.0",
  "private": true,
  "description": "A Vue.js project (boilerplate) template.",
  "author": "Jon LaBelle",
  "bugs": {
    "url": "https://github.com/jonlabelle/vue-project-template/issues"
  },
  "homepage": "https://github.com/jonlabelle/vue-project-template",
  "keywords": [
    "vue",
    "project-template",
    "template"
  ],
  "license": "MIT",
  "repository": "jonlabelle/vue-project-template",
  "scripts": {
    "serve": "vue-cli-service serve --mode development --port 8080",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint --fix",
    "build-dev": "vue-cli-service build --mode development",
    "build-report": "vue-cli-service build --mode production --report",
    "format": "prettier . --write '!**/*.{yml,md}'",
    "format-check": "prettier . --check '!**/*.{yml,md}'",
    "format-check-debug": "prettier . --check --loglevel debug '!**/*.{yml,md}'",
    "test": "vue-cli-service test:unit",
    "docker": "docker run --rm --name vue-project-template-dev --publish 8080:80 vue-project-template:dev",
    "docker-build": "docker build --no-cache --file .docker/Dockerfile --tag vue-project-template:dev ."
  },
  "dependencies": {
    "axios": "^0.25.0",
    "bootstrap": "^5.1.3",
    "core-js": "^3.8.3",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "@fullhuman/vue-cli-plugin-purgecss": "~4.1.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "babel-jest": "^27.4.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0"
  }
}

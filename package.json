{
  "name": "vue-project-template",
  "version": "0.1.0",
  "private": true,
  "description": "A Vue.js project (boilerplate) template.",
  "author": "Jon LaBelle",
  "bugs": {
    "url": "https://github.com/jonlabelle/vue-project-template/issues"
  },
  "homepage": "https://github.com/jonlabelle/vue-project-template",
  "keywords": [
    "vue",
    "project-template",
    "template"
  ],
  "license": "MIT",
  "repository": "jonlabelle/vue-project-template",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "docker-build": "docker build --tag vue-project-template:latest-dev .",
    "docker-run": "docker run -it --rm --name vue-project-template -p 8080:80 vue-project-template:latest-dev",
    "docker-run-detach": "docker run --detach --rm --name vue-project-template -p 8080:80 vue-project-template:latest-dev"
  },
  "dependencies": {
    "axios": "^0.21.3",
    "bootstrap": "^5.1.0",
    "core-js": "^3.17.2",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.4",
    "@vue/cli-plugin-babel": "~5.0.0-beta.2",
    "@vue/cli-plugin-eslint": "~5.0.0-beta.2",
    "@vue/cli-plugin-router": "~5.0.0-beta.2",
    "@vue/cli-service": "~5.0.0-beta.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.17.0",
    "prettier": "^2.3.2",
    "sass": "^1.39.0",
    "sass-loader": "^12.1.0",
    "vue-template-compiler": "^2.6.14"
  }
}
